#include <assert.h>
#include <stdio.h>
#include "../include/TEST.h"

extern int esBisiesto(int anio);

void PROBAR() {
    assert(esBisiesto(1980) == 1);
    assert(esBisiesto(1981) == 0);
    assert(esBisiesto(1984) == 1);
