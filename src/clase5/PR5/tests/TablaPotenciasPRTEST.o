#include <assert.h>
#include <stdio.h>
#include <stdlib.h>      
#include "../include/TEST.h"

extern void TablaPotencias(int n, int a, int b);

void PROBAR() {
    int i;
    float resultado;

    // Test case 1: Base 2, range -2 to 3
    printf("Testing TablaPotencias with base 2 from -2 to 3:\n");
    TablaPotencias(2, -2, 3);
    PRINT_TEST("Expected output for base 2 from -2 to 3\n");

    // Test case 2: Base -3, range 0 to 4
    printf("Testing TablaPotencias with base -3 from 0 to 4:\n");
    TablaPotencias(-3, 0, 4);
    PRINT_TEST("Expected output for base -3 from 0 to 4\n");

    // Test case 3: Base 5, range -1 to 2
    printf("Testing TablaPotencias with base 5 from -1 to 2:\n");
    TablaPotencias(5, -1, 2);
    PRINT_TEST("Expected output for base 5 from -1 to 2\n");

    // Test case 4: Base 0, range 1 to 3 (should handle indeterminate cases)
    printf("Testing TablaPotencias with base 0 from 1 to 3:\n");
    TablaPotencias(0, 1, 3);
    PRINT_TEST("Expected output for base 0 from 1 to 3\n");

    // Test case 5: Base 1, range -5 to 5
    printf("Testing TablaPotencias with base 1 from -5 to 5:\n");
    TablaPotencias(1, -5, 5);
    PRINT_TEST("Expected output for base 1 from -5 to 5\n");
}